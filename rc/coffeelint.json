{
  "indentation": {
    "value": 2,
    "level": "error"
  },
  "no_tabs": {
    "level": "error"
  },
  "max_line_length": {
    "value": 90,
    "level": "warn"
  },
  "line_endings": {
    "value": "unix",
    "level": "warn"
  },
  "no_trailing_whitespace": {
    "level": "error"
  },
  "no_empty_param_list": {
    "level": "error"
  },
  "arrow_spacing": {
    "level": "error"
  },
  "no_interpolation_in_single_quotes": {
    "level": "error"
  },
  "no_debugger": {
    "level": "error"
  },
  "colon_assignment_spacing": {
    "spacing": {
      "left": 0,
      "right": 1
    },
    "level": "error"
  },
  "braces_spacing": {
    "spaces": 0,
    "level": "error"
  },
  "no_throwing_strings": {
    "level": "error"
  },
  "no_implicit_parens": {
    "level": "ignore"
  },
  "no_implicit_braces": {
    "level": "ignore"
  },
  "camel_case_classes": {
    "level": "error"
  }
}
